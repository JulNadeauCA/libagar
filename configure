{
	va_list ap;
	struct syslog_data sdata = SYSLOG_DATA_INIT;
	va_start(ap, fmt);
	vsyslog_r(LOG_DEBUG, &sdata, fmt, ap);
	va_end(ap);
}
int
main(int argc, char *argv[])
{
	foofn("foo %d", 1);
	return (0);
}

