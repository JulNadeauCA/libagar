			if [ -x "${i}/cc" ]; then
				if [ -f "${i}/cc" ]; then
					OBJC="${i}/cc"
					break
				fi
			elif [ -x "${i}/gcc" ]; then
				if [ -f "${i}/gcc" ]; then
					OBJC="${i}/gcc"
					break
				fi
			fi
		done
		if [ "$OBJC" = "" ]; then
			echo "*"
			echo "* Unable to find an Objective-C compiler in PATH. You may need"
			echo "* to set the CC or OBJC environment variable."
