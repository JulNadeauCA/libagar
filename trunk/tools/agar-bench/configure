	cat << EOT > conftest.c
#include <sys/types.h>
#include <stdio.h>
#include <machine/pctr.h>
int
main(int argc, char *argv[])
{
	pctrval v;

	v = rdtsc();
	return (0);
}

EOT
	echo "$CC $CFLAGS $TEST_CFLAGS -o $testdir/conftest conftest.c" >>config.log
	$CC $CFLAGS $TEST_CFLAGS -o $testdir/conftest conftest.c 2>>config.log
	if [ $? != 0 ]; then
		echo "-> failed ($?)" >> config.log
		MK_COMPILE_STATUS="FAIL($?)"
	fi
fi
if [ "${MK_COMPILE_STATUS}" = "OK" ]; then
echo "yes"
echo "yes" >> config.log
HAVE_PCTR="yes"
echo "#ifndef HAVE_PCTR" > config/have_pctr.h
echo "#define HAVE_PCTR \"$HAVE_PCTR\"" >> config/have_pctr.h
echo "#endif" >> config/have_pctr.h
echo "hdefs[\"HAVE_PCTR\"] = \"$HAVE_PCTR\"" >>configure.lua
else
echo "no"
echo "no" >> config.log
HAVE_PCTR="no"
echo "#undef HAVE_PCTR" >config/have_pctr.h
echo "hdefs[\"HAVE_PCTR\"] = nil" >>configure.lua
fi;
if [ "${cache}" != "" ]; then
	echo "$HAVE_PCTR" > ${cache}/ctest-HAVE_PCTR
	echo $MK_COMPILE_STATUS > ${cache}/ctest-status-HAVE_PCTR
fi
rm -f conftest.c $testdir/conftest$EXECSUFFIX
CFLAGS="$CFLAGS -D_USE_AGAR_MATH"
CXXFLAGS="$CXXFLAGS -D_USE_AGAR_MATH"
echo "CFLAGS=$CFLAGS" >>Makefile.config
echo "mdefs[\"CFLAGS\"] = \"$CFLAGS\"" >>configure.lua
echo "CXXFLAGS=$CXXFLAGS" >>Makefile.config
echo "mdefs[\"CXXFLAGS\"] = \"$CXXFLAGS\"" >>configure.lua
CFLAGS="$CFLAGS -DHAVE_RDTSC"
CXXFLAGS="$CXXFLAGS -DHAVE_RDTSC"
echo "CFLAGS=$CFLAGS" >>Makefile.config
echo "mdefs[\"CFLAGS\"] = \"$CFLAGS\"" >>configure.lua
echo "CXXFLAGS=$CXXFLAGS" >>Makefile.config
echo "mdefs[\"CXXFLAGS\"] = \"$CXXFLAGS\"" >>configure.lua
