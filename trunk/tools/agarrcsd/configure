		if [ -x "${i}/cc" ]; then
			if [ -f "${i}/cc" ]; then
				CC="${i}/cc"
				break
			fi
		elif [ -x "${i}/gcc" ]; then
			if [ -f "${i}/gcc" ]; then
				CC="${i}/gcc"
				break
			fi
		fi
	done
	if [ "$CC" = "" ]; then
		echo "Unable to find a C compiler in PATH. Please set your compiler"
		echo "explicitely with the CC environment variable."
		echo "Unable to find a C compiler in PATH." >> config.log
